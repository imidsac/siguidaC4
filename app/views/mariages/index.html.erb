      <th><%= model_class.human_attribute_name(:nom_prenom_mere_mari) %></th>
      <th><%= model_class.human_attribute_name(:nom_prenom_tm_mari) %></th>
      <th><%= model_class.human_attribute_name(:date_nai_tm_mari) %></th>
      <th><%= model_class.human_attribute_name(:lieu_nai_tm_mari) %></th>
      <th><%= model_class.human_attribute_name(:domicil_tm_mari) %></th>
    -->
      <th><%= model_class.human_attribute_name(:nom_marie) %></th>
      <th><%= model_class.human_attribute_name(:prenom_marie) %></th>
      <!--
      <th><%= model_class.human_attribute_name(:date_nai_marie) %></th>
      <th><%= model_class.human_attribute_name(:lieu_nai_marie) %></th>
      <th><%= model_class.human_attribute_name(:profession_marie) %></th>
      <th><%= model_class.human_attribute_name(:domicil_marie) %></th>
      <th><%= model_class.human_attribute_name(:nom_prenom_pere_marie) %></th>
      <th><%= model_class.human_attribute_name(:nom_prenom_mere_marie) %></th>
      <th><%= model_class.human_attribute_name(:consente_parent) %></th>
      <th><%= model_class.human_attribute_name(:nom_prenom_tm_marie) %></th>
      <th><%= model_class.human_attribute_name(:date_nai_tm_marie) %></th>
      <th><%= model_class.human_attribute_name(:lieu_nai_tm_marie) %></th>
      <th><%= model_class.human_attribute_name(:domicil_tm_marie) %></th>
      <th><%= model_class.human_attribute_name(:dot) %></th>
      <th><%= model_class.human_attribute_name(:nom_prenom_officie) %></th>
      <th><%= model_class.human_attribute_name(:qualite_officie) %></th>
    -->
      <th><%= model_class.human_attribute_name(:date_officie) %></th>
     *
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @mariages.each do |mariage| %>
      <tr>
        <td><%= link_to mariage.id, mariage_path(mariage) %></td>
        <td><%= mariage.n_volet %></td>
        <td><%= mariage.nom_mari %></td>
        <td><%= mariage.prenom_mari %></td>
        <!--
        <td><%= mariage.date_nai_mari %></td>
        <td><%= mariage.lieu_nai_mari %></td>
        <td><%= mariage.profession_mari %></td>
        <td><%= mariage.domicil_mari %></td>
        <td><%= mariage.nom_prenom_pere_mari %></td>
        <td><%= mariage.nom_prenom_mere_mari %></td>
        <td><%= mariage.nom_prenom_tm_mari %></td>
        <td><%= mariage.date_nai_tm_mari %></td>
        <td><%= mariage.lieu_nai_tm_mari %></td>
        <td><%= mariage.domicil_tm_mari %></td>
      -->
        <td><%= mariage.nom_marie %></td>
        <td><%= mariage.prenom_marie %></td>
        <!--
        <td><%= mariage.date_nai_marie %></td>
        <td><%= mariage.lieu_nai_marie %></td>
        <td><%= mariage.profession_marie %></td>
        <td><%= mariage.domicil_marie %></td>
        <td><%= mariage.nom_prenom_pere_marie %></td>
        <td><%= mariage.nom_prenom_mere_marie %></td>
        <td><%= mariage.consente_parent %></td>
        <td><%= mariage.nom_prenom_tm_marie %></td>
        <td><%= mariage.date_nai_tm_marie %></td>
        <td><%= mariage.lieu_nai_tm_marie %></td>
        <td><%= mariage.domicil_tm_marie %></td>
        <td><%= mariage.dot %></td>
        <td><%= mariage.nom_prenom_officie %></td>
        <td><%= mariage.qualite_officie %></td>
      -->
        <td><%= mariage.date_officie %></td>
       
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_mariage_path(mariage), :class => 'btn btn-primary btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      mariage_path(mariage),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-danger btn-xs' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_mariage_path,
            :class => 'btn btn-primary' %>
