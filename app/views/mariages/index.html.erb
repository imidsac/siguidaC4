<%- model_class = Mariage -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:n_volet) %></th>
      <th><%= model_class.human_attribute_name(:nom_mari) %></th>
      <th><%= model_class.human_attribute_name(:prenom_mari) %></th>
